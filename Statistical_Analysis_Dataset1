top_indices <- order(data_athens, decreasing = TRUE)[1:10]
top_rv_values <- data_athens[top_indices]
top_quarters <- y[top_indices, 1]  
top_10_rv <- data.frame(Quarter = top_quarters, RV = top_rv_values)
print(top_10_rv)

top_indices <- order(data_barcelona, decreasing = TRUE)[1:10]
top_rv_values <- data_barcelona[top_indices]
top_quarters <- y[top_indices, 1]  
top_10_rv <- data.frame(Quarter = top_quarters, RV = top_rv_values)
print(top_10_rv)


